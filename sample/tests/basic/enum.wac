

fn __test_enum() {
    Assert(BasicEnum.A is BasicEnum.A)
    Assert(BasicEnum.B is BasicEnum.B)
    Assert(BasicEnum.B is not BasicEnum.C)
}

enum BasicEnum {
    A,
    B,
    C,
}

fn __test_enum_str() {
    AssertEq(repr(BasicEnum.A), 'BasicEnum.A')
    AssertEq(repr(BasicEnum.B), 'BasicEnum.B')
    AssertEq(repr(BasicEnum.C), 'BasicEnum.C')
    AssertEq(str(BasicEnum.A), 'A')
    AssertEq(str(BasicEnum.B), 'B')
    AssertEq(str(BasicEnum.C), 'C')
}
