
fn __test_int_eq() {
    Assert(5 == 5)
    Assert(5 != 7)
}

fn __test_arith_misc() {
    AssertSame(5 + 5, 10)

    # 5.0 should not be the same as 5
    Assert(5 / 1 is not 5)

    AssertSame(5 // 1, 5)

    AssertSame(type(5), i32)
    AssertSame(type(5 // 1), i32)
    AssertSame(type(5 / 1), f32)

    AssertEq(5 % 3, 2)
}

fn __test_arith_64bit() {
    var x i64 = 123
    var y i64 = 234
    Assert(x + y is (357).(i64))
}
